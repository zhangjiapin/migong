		#include <stdio.h>
#include <stdlib.h>
#include<windows.h>
#include<conio.h>
int main()
{
	char str[10][17] =
	{
		"**#*****########",
		"#*#*#######****#",
		"#***###*****####",
		"###**#**########",
		"##***#####*****#",
		"##*###*****#####",
		"#**###*#####***#",
		"#*####**######*#",
		"#*#####*####***#",
		"#************#*S"
	};
	int ch;
	COORD pos;
	pos.X = 0;
    pos.Y = 0; 
	while (1)
	{
		system("cls");
		char (*p)[17]=str;
		int i, j;
		for (i = 0; i < 10; i++,p++)
		{
			for (j = 0; j < 17; j++)
				printf("%c", p[0][j]);
			printf("\n");
		}
		SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), pos);
		printf("A");
		ch = _getch();
		if (str[pos.Y][pos.X] == 'S'){printf("¹§Ï²¹ý¹Ø"); break;}
		switch (ch)
		{
		case 'w':
			pos.Y--;
      if (pos.Y < 1) 
				pos.Y = 0;
			if(str[pos.Y][pos.X] == '#')
				pos.Y++;
			break;
		case 's':
			pos.Y++;
			if (pos.Y >= 9) 
				pos.Y = 9;
			if(str[pos.Y][pos.X] == '#')
				pos.Y--;
			break;
		case 'a':
			pos.X--;
			if (pos.X < 1)
				pos.X = 0;
			if(str[pos.Y][pos.X] == '#')
				pos.X++;
			break;
		case 'd':
			pos.X++;
			if (pos.X >= 15) 
				pos.X = 15;
			if(str[pos.Y][pos.X] == '#')
				pos.X--;
			break;
		}
	}
	return 0;
}
